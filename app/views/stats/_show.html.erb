<td class="game-row-data">
  <%= game %>
</td>
<% if stat.winner %>
  <td class="row-data no-edit">
    <%= stat.winner.name %>
  </td>
<% end %>
<% if stat.most_points %>
  <td class="row-data no-edit">
    <%= stat.most_points.name %>
  </td>
<% end %>
<% if stat.most_assists %>
  <td class="row-data no-edit">
    <%= stat.most_assists.name %>
  </td>
<% end %>
<% if stat.most_rebounds %>
  <td class="row-data no-edit">
    <%= stat.most_rebounds.name %>
  </td>
<% end %>
